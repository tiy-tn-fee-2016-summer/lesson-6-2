<div class="image-grid">
  <div class="image-grid__item">
    {{!-- model.forEach((image) => {}) --}}
    {{#each model.data as |image|}}
      {{#unless image.is_album}}
        <figure class="post">
          <div class="post__frame">
            <img src="{{ image.link }}" alt="">
          </div>
          <figcaption>
            <p>{{ image.title }}</p>
          </figcaption>
        </figure>
      {{else}}
        <figure class="post">
          <div class="post__frame">
            <h2>Image not found</h2>
          </div>
          <figcaption>
            <p>{{ image.title }}</p>
          </figcaption>
        </figure>
      {{/unless}}
    {{/each}}
  </div>
</div>

{{outlet}}
